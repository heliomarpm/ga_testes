  "homepage": "https://github.com/heliomarpm/ga_testes#readme",
  "build": {
    "appId": "com.ga_testes.app",
    "productName": "ga_testes",
    "icon": "path/to/icons/dir",
    "directories": {
      "buildResources": "assets/images/build"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "linux": {
      "category": "Utility",
      "target": "AppImage"
    },
    "win": {
      "target": "portable"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "prebuild": "node prebuild.js",
    "build": "electron-builder -wml",
    "build-win": "npm run prebuild && electron-builder -w",
    "build-win32": "npm run prebuild && electron-builder -w \"--ia32\"",
    "build-mac": "npm run prebuild && electron-builder -m",
    "build-linux": "npm run prebuild && electron-builder -l",
    "dist": "build"
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0"
  }
}
